{
  "rewrites": [
    {
      "source": "/api/auth/:path*",
      "destination": "https://microjam-backend-8l29.vercel.app/auth/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://microjam-backend-8l29.vercel.app/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}